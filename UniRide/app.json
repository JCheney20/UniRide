{
  "expo": {
    "name": "UniRide",
    "slug": "UniRide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "uniride",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "MBXAccessToken": "pk.eyJ1Ijoiam1jaGVuZXkiLCJhIjoiY21mcXdsemRvMDNhNTJscXltdHN6NHcweSJ9.Ie9ZGbjVyitsJwIJ96trjQ"
      },
      "bundleIdentifier": "com.anonymous.UniRide"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "mapbox": {
          "accessToken": "pk.eyJ1Ijoiam1jaGVuZXkiLCJhIjoiY21mcXdsemRvMDNhNTJscXltdHN6NHcweSJ9.Ie9ZGbjVyitsJwIJ96trjQ"
        }
      },
      "package": "com.anonymous.UniRide"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
